<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>@ViewBag.Title - My ASP.NET</title>
    <link rel="icon" href="~/Images/cloud-rainbow-icon64.png" />
    @Styles.Render("~/Content/css")
    @Scripts.Render("~/bundles/modernizr")
</head>
<body>
    <div class="container">
        <div class="navbar-header" style="height: 90px;margin-top:32px;">
            <a class="navbar-brand" href="#">
                <!--f_img
                <span>file imaging service</span>-->
                <img src="~/Images/cloud-rainbow-icon64.png" width="343px">
            </a>
        </div>
    </div>
    <hr style="margin:0;">
    <div class="container body-content">
        @RenderBody()
        <hr />
        <footer>
            <p>&copy; @DateTime.Now.Year - My ASP.NET Application</p>
        </footer>
    </div>
    @Scripts.Render("~/bundles/jquery")
    @Scripts.Render("~/bundles/bootstrap")
    @RenderSection("scripts", required: false)
</body>
</html>